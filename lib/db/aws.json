{
  "title": "AWS Secrets & Enumeration",
  "checks": [{
      "title": "AWS Secrets",
      "regex": "AKIA[0-9A-Z]{16}",
      "severity": 10
    },
    {
      "title": "AWS ARN",
      "regex": "arn:aws:.*",
      "severity": 1
    },
    {
      "title": "AWS EC2 Internal",
      "regex": ".*\\.compute\\.internal",
      "severity": 1
    },
    {
      "title": "AWS EC2 External 001",
      "regex": "ec2-.+?\\.compute\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS EC2 External 002",
      "regex": "ec2-.+?\\.compute-1\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS ELB 001",
      "regex": ".*\\.elb\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS ELB 002",
      "regex": ".*\\.elb\\..+?\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS RDS",
      "regex": ".*\\.rds\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS ElasticCache",
      "regex": ".*\\.cache\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS S3 Endpoint 001",
      "regex": ".*\\.s3\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS S3 Endpoint 002",
      "regex": ".*\\.s3-website.+?\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS S3 Bucket",
      "regex": "s3://[a-z0-9-.]+",
      "severity": 1
    },
    {
      "title": "AWS CloudFront",
      "regex": ".*\\.cloudfront\\.net",
      "severity": 1
    },
    {
      "title": "AWS API Gateway",
      "regex": "execute-api\\..+?\\.amazonaws\\.com",
      "severity": 1
    }
  ]
}
