{
  "title": "AWS Secrets & Enumeration",
  "checks": [{
      "title": "AWS Secrets",
      "regex": "AKIA[0-9A-Z]{16}",
      "severity": 10
    },
    {
      "title": "AWS ARN",
      "regex": "arn:aws:[\\w.-:]+",
      "severity": 1
    },
    {
      "title": "AWS EC2 Internal",
      "regex": "[\\w.-]+\\.compute\\.internal",
      "severity": 1
    },
    {
      "title": "AWS EC2 External 001",
      "regex": "ec2-[\\w.-]+?\\.compute\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS EC2 External 002",
      "regex": "ec2-[\\w.-]+?\\.compute-1\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS ELB 001",
      "regex": "[\\w.-]+\\.elb\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS ELB 002",
      "regex": "[\\w.-]+\\.elb\\.[\\w.-]+?\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS RDS",
      "regex": "[\\w.-]+\\.rds\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS ElasticCache",
      "regex": "[\\w.-]+\\.cache\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS S3 Endpoint 001",
      "regex": "[\\w.-]+\\.s3\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS S3 Endpoint 002",
      "regex": "[\\w.-]+\\.s3-website[\\w.-]+?\\.amazonaws\\.com",
      "severity": 1
    },
    {
      "title": "AWS S3 Bucket",
      "regex": "s3://[a-z0-9-.]+",
      "severity": 1
    },
    {
      "title": "AWS CloudFront",
      "regex": "[\\w.-]+\\.cloudfront\\.net",
      "severity": 1
    },
    {
      "title": "AWS API Gateway",
      "regex": "execute-api\\.[\\w.-]+?\\.amazonaws\\.com",
      "severity": 1
    }
  ]
}
